/* pages/cation/cation.wxss */
.tit{
	color: #1F1F1F;
	font-size: 20px;
	width: 100%;
	height: 50px;
	line-height: 50px;
	text-align: center;
	position: fixed;
	top: 0;
	background-color: #fff;
	z-index: 999;
}


.con_box{
	margin-top: 50px;
	width: 90%;
	padding: 0 5%;
	/* background-color: skyblue; */
}
.con_tit{
	width: 98%;
	font-size: 15px;
	height: 30px;
	padding: 0 5px;
	line-height: 30px;
	border-left: 3px solid #3CD4BB;
}

.con_conBox{
	width: 100%;
}
.con_conBox::after{
	content: "";
	display: block;
	clear: both;
}
.con_con{
	width: 15%;
	height: 70px;
	text-align: center;
	padding: 0 5%;
	margin: 5px 0;
	float: left;
}
.con_con image{
	width: 50px;
	height: 50px;
}
.con_con view{
	width: 100%;
	text-align: center;
	height: 20px;
	line-height: 20px;
	font-size: 10px;
}